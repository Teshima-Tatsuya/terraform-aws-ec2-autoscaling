{
    "agent": {
      "metrics_collection_interval": 10
    },
    "metrics": {
      "namespace": "EC2",
      "metrics_collected": {
        "mem": {
          "measurement": ["mem_available_percent"]
        }
      },
      "aggregation_dimensions": [["InstanceId", "InstanceType"], ["InstanceId"]]
    }
  }